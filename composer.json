{
  "name": "voyager-blocker",
  "description": "Hook for Voyager that allows you to block access to admin side with ip's.",
  "keywords": ["laravel", "hooks", "voyager"],
  "tags": ["voyager"],
  "license": "MIT",
  "authors": [
        {
            "name": "David",
            "email": "davo_bidzian@mail.ru"
        }
  ],
  "require": {
    "larapack/hooks": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "VoyagerBlocker\\": "src/"
    }
  },
  "extra": {
    "hook": {
      "providers": [
        "VoyagerBlocker\\BlockerServiceProvider"
      ]
    }
  }
}